<import src="/components/declare/declare.wxml" />



<template name="list">
	<block wx:if="{{list.items.length>0}}">
		<!-- 推荐页的文章 -->
		<block wx:if="{{list._id==='topScoreds'}}">
			<!-- autoplay="true"  -->
