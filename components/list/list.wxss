.itemBigBox{
    width: 690rpx;
    height:500rpx;
    margin: 15rpx 30rpx;
    
}
.videoTitle{
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    width:410rpx;
    line-height:1.0;
    font-size: 32rpx;
    color: #333;
    font-size: 34rpx;
}
.fontShadow {
    height: 90rpx;
    width:690rpx;
    opacity: 0.5;
    background: -webkit-linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,1));/* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,1)); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,1)); /* Firefox 3.6 - 15 */
    background: linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,1));
}

