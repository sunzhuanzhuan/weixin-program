    background:#fff;
    
   
}
.nowrapTwo{
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    width: 410rpx;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    line-height:1.5
}
.nowrapOne{
    overflow: hidden;
