.fixed{
    position: fixed;
    bottom: 0;
    width: 100%;
    line-height: 94rpx;
    background: #ffffff;
    border-top:2rpx solid #efefef;
}
.floatShadow{
    height:30rpx ;
    opacity:0.04;
    background: -webkit-linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,3));/* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,3)); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,3)); /* Firefox 3.6 - 15 */
    background: linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,3));
}
